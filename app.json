    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAfssjDeBK4IVUhtopOWqXHmseWkjouBoY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app.png",
